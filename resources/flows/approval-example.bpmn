    id: 'magic', 
    result: {
        done: true,
        error: false,
        skipped: false,
        statuscode: 200,
        statusmessage: 'Let the magic begin!'
    }
});</camunda:script>
          </camunda:outputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0kx401p</bpmn:incoming>
      <bpmn:outgoing>Flow_1c8fgoj</bpmn:outgoing>
